{
  "argv": [
   "python",
   "-m",
   "ipykernel_launcher",
   "-f",
   "{connection_file}"
  ],
  "display_name": "Glue PySpark",
  "language": "python",
  "env": {
    "SPARK_HOME": "/usr/local/share/applications/spark-home",
    "SPARK_CONF_DIR": "/usr/local/share/applications/aws-glue-libs/conf",
    "PYTHONPATH": "/usr/local/share/applications/spark-home/python/:/usr/local/share/applications/spark-home/python/lib/py4j-0.10.7-src.zip:/usr/local/share/applications/aws-glue-libs/PyGlue.zip",
    "PYSPARK_PYTHON": "python3"
  }
}
